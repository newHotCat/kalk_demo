  "postcss": {
    "parser": "postcss-scss",
    "plugins": {
      "autoprefixer": {},
      "postcss-import": {},
      "postcss-calc": {},
      "precss": {},
      "postcss-px-to-viewport": {
        "viewportWidth": 750,
        "selectorBlackList": [
          "van"
        ]
      }
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
